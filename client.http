@host = 127.0.0.1
// localhost: 3000 | docker: 3030
@port = 3000
@uri = http://{{host}}:{{port}}/v1
@contentType = application/json

@userLoginEmail = mail@koakh.com
@userLoginPassword = 12345678

### ping
// @name ping

GET {{uri}}/app/ping HTTP/1.1
Content-Type: {{contentType}}

### userLogin
// @name userLogin

POST {{uri}}/users/login HTTP/1.1
Content-Type: {{contentType}}

{
  "user": {
    "email": "{{userLoginEmail}}",
    "password": "{{userLoginPassword}}"
  }
}
